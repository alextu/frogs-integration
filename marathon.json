{
    "container": {
        "docker": {
            "image": "artifactory-lb.marathon.mesos:5001/frogs-integration:FROGS_VERSION",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8090
                }
            ],
            "privileged": false
        },
        "type": "DOCKER"
    },
    "acceptedResourceRoles": ["slave_public"],
    "cpus": 0.5,
    "id": "frogs",
    "instances": 1,
    "mem": 512,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}