{
    "container": {
        "docker": {
            "image": "artifactory-lb.marathon.mesos:5001/frogs-integration:FROGS_VERSION",
            "network": "BRIDGE",
            "port_mappings": [
                {
                    "container_port": 80,
                    "protocol": "tcp"
                }
            ],
            "privileged": false,
            "force_pull_image": false
        },
        "type": "DOCKER"
    },
    "acceptedResourceRoles": ["slave_public"],
    "cpus": 0.1,
    "id": "frogs",
    "instances": 1,
    "labels": {
        "HAPROXY_0_VHOST": "ELB_HOSTNAME",
        "HAPROXY_GROUP": "external"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}